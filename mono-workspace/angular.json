{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"cli": {
		"analytics": false
	},
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"host-app": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/host-app",
			"sourceRoot": "projects/host-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/host-app",
						"index": "projects/host-app/src/index.html",
						"main": "projects/host-app/src/main.ts",
						"polyfills": "projects/host-app/src/polyfills.ts",
						"tsConfig": "projects/host-app/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/host-app/src/favicon.ico",
							"projects/host-app/src/assets"
						],
						"styles": [
							"projects/host-app/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/host-app/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/host-app/src/environments/environment.ts",
									"with": "projects/host-app/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"extraWebpackConfig": "projects/host-app/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "host-app:build",
						"port": 4100,
						"publicHost": "http://localhost:4100",
						"extraWebpackConfig": "projects/host-app/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "host-app:build:production",
							"extraWebpackConfig": "projects/host-app/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "host-app:build",
						"extraWebpackConfig": "projects/host-app/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/host-app/src/test.ts",
						"polyfills": "projects/host-app/src/polyfills.ts",
						"tsConfig": "projects/host-app/tsconfig.spec.json",
						"karmaConfig": "projects/host-app/karma.conf.js",
						"assets": [
							"projects/host-app/src/favicon.ico",
							"projects/host-app/src/assets"
						],
						"styles": [
							"projects/host-app/src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/host-app/tsconfig.app.json",
							"projects/host-app/tsconfig.spec.json",
							"projects/host-app/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/host-app/e2e/protractor.conf.js",
						"devServerTarget": "host-app:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "host-app:serve:production"
						}
					}
				}
			}
		},
		"mfe-app": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/mfe-app",
			"sourceRoot": "projects/mfe-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/mfe-app",
						"index": "projects/mfe-app/src/index.html",
						"main": "projects/mfe-app/src/main.ts",
						"polyfills": "projects/mfe-app/src/polyfills.ts",
						"tsConfig": "projects/mfe-app/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/mfe-app/src/favicon.ico",
							"projects/mfe-app/src/assets"
						],
						"styles": [
							"projects/mfe-app/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/mfe-app/src/environments/environment.ts",
									"with": "projects/mfe-app/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "mfe-app:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "mfe-app:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "mfe-app:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/mfe-app/src/test.ts",
						"polyfills": "projects/mfe-app/src/polyfills.ts",
						"tsConfig": "projects/mfe-app/tsconfig.spec.json",
						"karmaConfig": "projects/mfe-app/karma.conf.js",
						"assets": [
							"projects/mfe-app/src/favicon.ico",
							"projects/mfe-app/src/assets"
						],
						"styles": [
							"projects/mfe-app/src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/mfe-app/tsconfig.app.json",
							"projects/mfe-app/tsconfig.spec.json",
							"projects/mfe-app/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/mfe-app/e2e/protractor.conf.js",
						"devServerTarget": "mfe-app:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "mfe-app:serve:production"
						}
					}
				}
			}
		}
	},
	"defaultProject": "host-app"
}